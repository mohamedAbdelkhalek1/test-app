<odoo>


    <record model="ir.ui.view" id="session_search_form_view">
      <field name="name"> session search</field>
      <field name="model">session.search</field>
      <field name="arch" type="xml">
        <form string="Session search">
                <group>
                    <field name="start_date"/>
                    <field name="end_date"/>
                </group>
                <footer>
                        <button string="Search" type="object" name="session_search_action" class="btn-primary"/>
                        <button string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
        </form>
      </field>
    </record>

    <record id="launch_session_search_wizard" model="ir.actions.act_window">
            <field name="name">Session Search</field>
            <field name="res_model">session.search</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="binding_model_id" ref="model_test_app_session"/>
    </record>

    <record id="session_search_action" model="ir.actions.act_window">
        <field name="name">session search action</field>
        <field name="res_model">test_app.session</field>
        <field name="view_mode">tree,form,calendar,graph,pivot,kanban</field>
        <field name="domain">[('start_date', '&gt;=', start), ('start_date', '&lt;=', end)]</field>
    </record>


</odoo>